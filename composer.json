{
  "name": "deh4eg/omnipay-luminor",
  "description": "Luminor gateway for omnipay library",
  "keywords": [
    "omnipay",
    "luminor",
    "payment",
    "gateway",
    "acquiring"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Smolakov",
      "email": "denissmolakovs@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnipay\\Luminor\\": "src/",
      "Omnipay\\Luminor\\Test\\": "tests/"
    }
  },
  "require": {
    "ext-json": "*",
    "php": "^7.2",
    "league/omnipay": "^3"
  },
  "require-dev": {
    "phpro/grumphp": "^1.3",
    "omnipay/tests": "^3",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.81"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts" : {
    "test": "./vendor/bin/grumphp run"
  }
}
